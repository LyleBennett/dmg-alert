{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "dmg/alert",
  "version": "0.1.0",
  "title": "Dmg Alert",
  "category": "widgets",
  "icon": "bell",
  "description": "A block to display an alert message.",
  "supports": {
    "html": false
  },
  "keywords": [
    "alert",
    "dmg"
  ],
  "textdomain": "dmg-alert",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "alertType": {
      "default": "notice",
      "enum": [
        "notice",
        "warning",
        "error"
      ]
    }
  }
}